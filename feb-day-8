class Solution {
public:
    int jump(vector<int>& nums) {
        int cur=0,move=0,res=0;
        for(int i=0;i<nums.size()-1;i++)
        { 
            cur=max(cur,i+nums[i]);
            if(i==move)
            {
                move=cur;
                res++;
            }
        }
        return res;
    }
};
